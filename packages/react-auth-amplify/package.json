{
  "name": "@marzee/react-auth-amplify",
  "version": "1.0.3",
  "description": "A React.js-Amplify Auth integration",
  "repository": {
    "type": "git",
    "url": "https://github.com/marzeelabs/tools.git",
    "directory": "packages/react-auth-amplify"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      },
      {
        "name": "dev",
        "prerelease": true
      }
    ]
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npx tsup src/index.ts --format esm,cjs --minify --dts --treeshake --clean",
    "prepublishOnly": "pnpm run build"
  },
  "author": "Karim Daghari",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.18.5",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "aws-amplify": "^5.3.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "optionalDependencies": {
    "next": "^13.5.5"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
